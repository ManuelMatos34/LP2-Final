  <!-- contact -->
  <div id="contact" class="contact">
    <div class="container-fluid padding_left2">
      <div class="white_color container">

        <div class="row g-0 align-items-center">
          <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
            <iframe src="https://www.google.com/maps/d/embed?mid=10X7bS78XoKQG-YoLD_fSnRblB78&hl=en&ehbc=2E312F" width="540" height="586"></iframe>
          </div>

          <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
            <form class="contact_bg" asp-controller="Home" asp-action="EnviarCorreoContactanos" method="post">
              <div class="row">
                <div class="col-md-12">
                  <div class="titlepage">
                    <h2 style="color: darkgreen;">Contáctanos</h2>
                  </div>
                  <div class="col-md-12">
                    <input class="contactus" placeholder="Tu Nombre..." type="text" name="nombre" required> 
                  </div>
                  <div class="col-md-12">
                    <input class="contactus" placeholder="Tu Correo..." type="text" name="desde" required>
                  </div>
                  <div class="col-md-12">
                    <input class="contactus" placeholder="Tu Celular..." type="text" name="telefono" required>
                  </div>
                  <div class="col-md-12">
                    <textarea class="textarea" placeholder="Mensaje..." type="text" name="mensaje" required></textarea>
                  </div>
                  <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                    <button type="submit" class="send">Enviar</button>
                  </div>
                </div>
               </div>
            </form>
          </div>
        </div>

      </div>
    </div>
